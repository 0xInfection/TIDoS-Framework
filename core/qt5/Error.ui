QTabWidget::tab-bar {
background-color: #130e13;
min-width: 21;
} 

QTabBar::tab {
min-width: 21;
background: #130e13; /*221922*/
border-top-left-radius: 2px;
border-top-right-radius: 2px;
spacing: 6;
padding: 2px;
}

QTabBar::tab:selected {
border: 1px solid #4a3e4a;
border-right-color: #130e13; 
background: #130e13;
}
QTabBar::tab:!selected {
border: 1px solid #221922;
