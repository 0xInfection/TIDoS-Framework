border-right-color: #4a3e4a; 
background: #221922;
}
QTabBar::tab:first:!selected {
border: 1px solid #130e13;
border-right-color: #4a3e4a; 
border-top-color: #4a3e4a; 
border-bottom-color: #221922;
background: #221922;
}
QTabBar::tab:last:!selected {
border: 1px solid #130e13;
border-right-color: #4a3e4a; 
border-bottom-color: #4a3e4a; 
border-top-color: #221922;
background: #221922;
}

